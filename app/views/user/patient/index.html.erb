<h1>ユーザーマイページ</h1>

<h1>治療院一覧</h1>
 <table>
      <tr>
        <th>治療院名</th>
        <th>住所</th>
        <th>電話番号</th>
        <th>詳細</th>
      </tr>
    <% @clinics.each do |clinic| %>
      <tr>
        <td><%= clinic.name %></td>
        <td><%= clinic.address %></td>
        <td><%= clinic.phone_number %></td>
        <td>link_to '詳細'</td>
      </tr>
    <% end %>
    </table>